{
  "connection": "default",
  "collectionName": "post",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "default": "",
      "type": "string",
      "required": true
    },
    "Image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "Content": {
      "default": "",
      "type": "text",
      "required": true
    },
    "Published Date": {
      "default": "",
      "type": "date",
      "required": true
    },
    "user": {
      "model": "user",
      "via": "posts",
      "plugin": "users-permissions"
    }
  }
}